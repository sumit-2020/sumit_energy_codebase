this->cq->insert(genWriteCMD(5, 0, 0xcc, AUTO_PRECHARGE::NO_AP));
this->cq->insert(genWaitCMD (4));
this->cq->insert(genBusDirCMD(BUSDIR::WRITE));
this->cq->insert(genWaitCMD (6));
this->cq->insert(genBusDirCMD(BUSDIR::READ));
this->cq->insert(genWaitCMD (5));
this->cq->insert(genRowCMD  (98, 0, MC_CMD::PRE, PRECHARGE::SINGLE));
this->cq->insert(genWaitCMD (5));
this->cq->insert(genRowCMD  (98, 0, MC_CMD::ACT, PRECHARGE::SINGLE));
this->cq->insert(genWaitCMD (5));
this->cq->insert(genReadCMD (0, 0, AUTO_PRECHARGE::NO_AP));
this->cq->insert(genWaitCMD (6));
this->cq->insert(genWriteCMD(5, 0, 0xcc, AUTO_PRECHARGE::NO_AP));
this->cq->insert(genWaitCMD (4));
this->cq->insert(genBusDirCMD(BUSDIR::WRITE));
this->cq->insert(genWaitCMD (6));
this->cq->insert(genBusDirCMD(BUSDIR::READ));
this->cq->insert(genWaitCMD (5));
this->cq->insert(genRowCMD  (99, 0, MC_CMD::PRE, PRECHARGE::SINGLE));
this->cq->insert(genWaitCMD (5));
this->cq->insert(genRowCMD  (99, 0, MC_CMD::ACT, PRECHARGE::SINGLE));
this->cq->insert(genWaitCMD (5));
this->cq->insert(genReadCMD (0, 0, AUTO_PRECHARGE::NO_AP));
this->cq->insert(genWaitCMD (6));
this->cq->insert(genWriteCMD(5, 0, 0xcc, AUTO_PRECHARGE::NO_AP));
this->cq->insert(genWaitCMD (4));
this->cq->insert(genBusDirCMD(BUSDIR::WRITE));
this->cq->insert(genWaitCMD (6));
this->cq->insert(genBusDirCMD(BUSDIR::READ));
this->cq->insert(genWaitCMD (5));
this->cq->insert(genRowCMD  (99, 0, MC_CMD::PRE, PRECHARGE::SINGLE));
this->cq->insert(genWaitCMD (5));
}

